{
  "name": "nicholaschun/payment-sdk-php",
  "description": "Package with services for handling all payment requests",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicholas Mamiya"
    }
  ],
  "require": {
    "spatie/laravel-data": "^3.7"
  },
  "require-dev": {
    "orchestra/testbench": "7.0",
    "phpunit/phpunit": "^9.6",
    "guzzlehttp/guzzle": "^7.7"
  },
  "autoload": {
    "psr-4": {
      "nicholaschun\\payment-sdk-php\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "nicholaschun\\payment-sdk-php\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests"
  },
  "extra": {
    "laravel": {
      "providers": [
        "nicholaschun\\payment-sdk-php\\PaymentSDKServiceProvider"
      ]
    }
  }
}